{
  "env": {
    "development": {
      "presets": [
        ["babel-preset-env", {
          "targets": {
            "node": "6.1"
          }
        }],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators",
        "transform-class-properties"
      ]
    },
    "test": {
      "presets": [
        ["babel-preset-env", {
          "targets": {
            "node": "6.1"
          }
        }],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators",
        "transform-class-properties"
      ]
    },
    "browser": {
      "presets": [
        ["babel-preset-env", {
          "targets": {
            "browsers": "last 3 versions, > 1%"
          },
          "modules": false,
          "loose": true
        }],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators",
        "transform-class-properties",
        "transform-proto-to-assign"
      ]
    },
    "module": {
      "presets": [
        ["babel-preset-env", {
          "targets": {
            "node": "6.1"
          },
          "modules": false
        }],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators",
        "transform-class-properties"
      ]
    }
  },
  "sourceMaps": true
}